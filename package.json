{
  "name": "utage",
  "version": "1.0.0",
  "description": "Event / party planning",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint src/.; exit 0",
    "fix": "eslint --fix src/.; exit 0",
    "preprodrun": "cp src/runtime-caching.js dist/runtime-caching.js && cp src/sw-toolbox.js dist/sw-toolbox.js && cp src/manifest.json dist/manifest.json",
    "prod": "cp node_modules/sw-toolbox/sw-toolbox.js src/sw-toolbox.js && webpack -p && npm run preprodrun && npm run precache",
    "prod:run": "cd dist && http-server",
    "precache": "sw-precache --root=dist --config=sw-precache-config.json",
    "test": "mocha --require babel-core/register --require ignore-styles ./test/**/**/*.js",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --compilers babel-core/register --require test/setup.js --recursive 'test/**/*.test.js' && open coverage/lcov-report/*.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhagio/utage.git"
  },
  "keywords": [
    "Node.js",
    "React.js",
    "Reactjs",
    "Firebase",
    "Mobile",
    "Progressive",
    "Web",
    "Redux",
    "React-Router",
    "ReactRouter",
    "mobile",
    "web",
    "google",
    "map",
    "uber",
    "responsive",
    "webpack"
  ],
  "author": "Yuichi Hagio <yhagio87@gmail.com> (http://github.com/yhagio)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yhagio/utage/issues"
  },
  "homepage": "https://github.com/yhagio/utage#readme",
  "dependencies": {
    "axios": "^0.12.0",
    "firebase": "^3.2.1",
    "immutable": "^3.8.1",
    "react": "^15.3.1",
    "react-dom": "^15.2.0",
    "react-google-maps": "^4.11.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.0.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "ignore-styles": "^4.0.0",
    "image-webpack-loader": "^2.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "sw-precache": "^4.0.0",
    "sw-toolbox": "^3.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
